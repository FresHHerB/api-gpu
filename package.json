{
  "name": "api-gpu",
  "version": "1.0.0",
  "description": "Video processing API with RunPod Serverless GPU auto-scaling",
  "main": "dist/orchestrator/index.js",
  "scripts": {
    "build": "npm run build:orchestrator",
    "build:orchestrator": "tsc -p tsconfig.orchestrator.json",
    "dev": "ts-node-dev --respawn --transpile-only src/orchestrator/index.ts",
    "start": "node dist/orchestrator/index.js",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "video",
    "processing",
    "ffmpeg",
    "gpu",
    "cuda",
    "runpod",
    "serverless",
    "auto-scaling",
    "orchestrator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@runware/sdk-js": "^1.1.47",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "joi": "^17.11.0",
    "playwright": "^1.40.0",
    "runpod-sdk": "^1.1.2",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
